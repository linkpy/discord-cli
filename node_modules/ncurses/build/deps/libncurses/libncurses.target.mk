libncurses: $(builddir)/ncurses.a

# Copy this to the static library output path.
$(builddir)/ncurses.a: TOOLSET := $(TOOLSET)
$(builddir)/ncurses.a: $(obj).target/deps/libncurses/ncurses.a FORCE_DO_CMD
	$(call do_cmd,copy)

all_deps += $(builddir)/ncurses.a
# Short alias for building this static library.
.PHONY: ncurses.a
ncurses.a: $(obj).target/deps/libncurses/ncurses.a $(builddir)/ncurses.a

# Add static library to "all" target.
.PHONY: all
all: $(builddir)/ncurses.a

